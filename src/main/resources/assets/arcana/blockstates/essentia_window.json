{
  "multipart": [
    {   "when": { "north": true },
      "apply": { "model": "arcana:block/tubes/essentia_window_side" }
    },
    {   "when": { "east": true },
      "apply": { "model": "arcana:block/tubes/essentia_window_side", "y": 90, "uvlock": true }
    },
    {   "when": { "south": true },
      "apply": { "model": "arcana:block/tubes/essentia_window_side", "y": 180, "uvlock": true }
    },
    {   "when": { "west": true },
      "apply": { "model": "arcana:block/tubes/essentia_window_side", "y": 270, "uvlock": true }
    },
    {   "when": { "up": true },
      "apply": { "model": "arcana:block/tubes/essentia_window_side", "x": 270, "uvlock": true }
    },
    {   "when": { "down": true },
      "apply": { "model": "arcana:block/tubes/essentia_window_side", "x": 90, "uvlock": true }
    },
    { "apply": { "model": "arcana:block/tubes/essentia_window_middle" }}
  ]
}
